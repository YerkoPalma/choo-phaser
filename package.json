{
  "name": "choo-phaser",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "standard --verbose | snazzy",
    "start": "budo -l -v -p 8080 -H 0.0.0.0 -d assets index.js -- -t sheetify",
    "build:js": "mkdir -p dist && browserify -t sheetify -p [ css-extract -o dist/bundle.css ] index.js -o dist/bundle.js",
    "build:html": "./node_modules/.bin/htmlminify -o dist/index.html index.html && cp dist/index.html dist/404.html",
    "build:assets": "cp -r assets/ dist/assets",
    "build": "npm run build:js && npm run build:html && npm run build:assets"
  },
  "keywords": [],
  "author": "Yerko Palma",
  "license": "MIT",
  "dependencies": {
    "choo": "^6.6.0",
    "choo-stt": "^0.1.0",
    "choo-tts": "^0.3.0",
    "lowdb": "^1.0.0",
    "lowdb-gh-adapter": "^1.0.0",
    "phaser": "^2.6.2",
    "sheetify": "^6.2.0",
    "tachyons": "^4.9.0"
  },
  "devDependencies": {
    "browserify": "^14.5.0",
    "budo": "^10.0.4",
    "choo-devtools": "^2.2.0",
    "css-extract": "^1.2.0",
    "html-minify": "^0.3.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "tinyify": "^2.3.0"
  }
}
